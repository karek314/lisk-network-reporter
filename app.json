[
  {
    "name"              : "lisk-network-reporter",
    "script"            : "app.js",
    "log_date_format"   : "YYYY-MM-DD HH:mm Z",
    "merge_logs"        : false,
    "watch"             : false,
    "max_restarts"      : 10,
    "exec_interpreter"  : "node",
    "exec_mode"         : "fork_mode",
    "env":
    {
      "NODE_ENV"        : "production",
      "RPC_HOST"        : "localhost",
      "RPC_PORT"        : "8000",
      "LISTENING_PORT"  : "8000",
      "INSTANCE_NAME"   : "PUT_INSTANCE_NAME_HERE",
      "CONTACT_DETAILS" : "PUT_CONTACT_DETAILS_HERE",
      "NETWORK_MODE"    : "main",
      "WS_SERVER"       : "ws://report.liskstats.net:3000",
      "WS_SECRET"       : "W3B.........",
      "IS_FORGING"      : "no",
      "VERBOSITY"       : 2
    }
  }
]
